{
    "name": "Bitbucket Extension",
    "version": "1.0",
    "description": "Bitbucket Extension!",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "activeTab",
		"storage"
    ]
}
